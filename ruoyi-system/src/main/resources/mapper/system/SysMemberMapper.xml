    <select id="queryCondition" parameterType="com.ruoyi.system.domain.SysMember" resultMap="BaseResultMap">
        select
            <include refid="BASE_COLUMN"></include>
        from sys_member
            <where>
                STATUS <![CDATA[<>]]> '0'
                <if test="name != null and name != ''">
                    NAME like concat('%',#{name},'%')
                </if>
                <if test="nickname != null and nickname != ''">
                    NICKNAME like concat('%',#{nickname},'%')
                </if>
            </where>
    </select>

