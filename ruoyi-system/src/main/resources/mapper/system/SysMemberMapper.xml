    <!--通过主键修改数据-->
    <update id="update" parameterType="com.ruoyi.system.domain.SysMember">
        update shiyi.sys_member
        <set>
            <if test="cardNum != null and cardNum != ''">
                CARD_NUM = #{cardNum},
            </if>
            <if test="name != null and name != ''">
                NAME = #{name},
            </if>
            <if test="nickname != null and nickname != ''">
                NICKNAME = #{nickname},
            </if>
            <if test="gender != null and gender != ''">
                GENDER = #{gender},
            </if>
            <if test="phone != null and phone != ''">
                PHONE = #{phone},
            </if>
            <if test="balance != null">
                BALANCE = #{balance},
            </if>
            <if test="additionalBalance != null">
                ADDITIONAL_BALANCE = #{additionalBalance},
            </if>
            <if test="sumOfTopUp != null">
                SUM_OF_TOP_UP = #{sumOfTopUp},
            </if>
            <if test="sumOfConsumption != null">
                SUM_OF_CONSUMPTION = #{sumOfConsumption},
            </if>
            <if test="remark != null and remark != ''">
                REMARK = #{remark},
            </if>
            <if test="createdBy != null and createdBy != ''">
                CREATED_BY = #{createdBy},
            </if>
            <if test="createdTime != null">
                CREATED_TIME = #{createdTime},
            </if>
            <if test="updatedBy != null and updatedBy != ''">
                UPDATED_BY = #{updatedBy},
            </if>
            <if test="updatedTime != null">
                UPDATED_TIME = #{updatedTime},
            </if>
            <if test="status != null and status != ''">
                STATUS = #{status},
            </if>
            <if test="spare2 != null and spare2 != ''">
                SPARE2 = #{spare2},
            </if>
            <if test="spare3 != null and spare3 != ''">
                SPARE3 = #{spare3},
            </if>
        </set>
        where ID = #{id}
    </update>

    <!--批量逻辑删除-->
    <update id="batchLogicDel">
        update sys_member sm
            <set>
                sm.STATUS = '0'
            </set>
        WHERE sm.ID IN
            <foreach collection="ids" index="index" item="id" open="(" close=")" separator=",">
                #{id}
            </foreach>
    </update>

